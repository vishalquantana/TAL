# Turso (managed libSQL) — required for production
TURSO_DATABASE_URL=
TURSO_AUTH_TOKEN=
# JWT secret — use a random string in production
JWT_SECRET=change-me-to-a-random-string

# Frontend URL — used for password reset links
FRONTEND_URL=http://localhost:3000

# AWS S3 — optional, falls back to local disk when not set
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=
AWS_REGION=
